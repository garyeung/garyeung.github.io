---
import CustomDate from './CustomDate.astro';

interface Props {
  title: string;
  url: string;
  pubDate: Date;
}

const {title, url, pubDate} = Astro.props;
---

<div class="">
  <a href={url} class="flex justify-between ">
  <h2 class="">{title}</h2>
  <p class="text-secondary">
    <CustomDate date={pubDate}/>
  </p>
  </a>
</div>